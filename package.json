    "ts-node": "3.3.0",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.1",
    "cors": "^2.8.5",
    "express": "^4.15.4",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.10",
    "tslint": "^6.1.3",
    "typeorm": "0.2.28"
